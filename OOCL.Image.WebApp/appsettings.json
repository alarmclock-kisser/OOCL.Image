{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiBaseUrl": "https://api.oocl.work/api",
  "DefaultDarkMode": true,
  "PreferredDevice": "Xeon",
  "ImagesLimit": 64,
  "TracksLimit": 4,
  "Kestrel": {
    "Endpoints": {
      "Https": { "Url": "https://0.0.0.0:7240" }
    }
  },
  "DefaultSelections": {
    "Kernel": "mandelbrot00",
    "Format": "png",
    "Unit": "MB"
  },
  "MaxLogLines": 4096,
  "CleanupPreviousLogs": false,
  "TimeoutSeconds": 120,
  "DefaultCompressionBits": 0,
  "UseCompressionMusLaw": true
}